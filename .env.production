# Production Environment Configuration
# Overrides defaults for production environment

# Database Configuration
DATABASE_URL=sqlite:///backend/orchestrator_prod.db

# Server Configuration
API_PORT=8002
FRONTEND_PORT=5176
ENVIRONMENT=production

# Production Settings
DEBUG=false
LOG_LEVEL=WARNING
RELOAD=false
BACKUP_ENABLED=true

# Performance Settings (optimized)
REQUEST_TIMEOUT=30
MAX_WORKERS=8
DATABASE_POOL_SIZE=10
DATABASE_ECHO=false

# Feature Flags (production)
ENABLE_INTEGRATIONS=true
ENABLE_ANALYTICS=true
ENABLE_CACHING=true

# Security Settings (strict)
SESSION_TIMEOUT=1800
RATE_LIMIT_ENABLED=true
CORS_ORIGINS=https://orchestrator.yourdomain.com

# API Keys - Set these in .env.local
# Use production API keys with appropriate limits
# See .env.example for template